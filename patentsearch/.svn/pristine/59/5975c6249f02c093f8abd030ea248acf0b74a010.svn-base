.dl
{
    width: 771px;
    height: 382px;
    margin: 40px auto 0 auto;
    border-width: 1px;
    border-color: #7BBEE7;
    background-color: White;
    border-style: ridge;
}
.dltitle
{
    height: 37px;
    overflow: hidden;
    line-height: 37px;
    background: url(../../images/d01.jpg) repeat-x top;
}
.dltitle span
{
    display: block;
    background: url(../../images/d02.jpg) no-repeat left top;
    height: 37px;
    font-size: 14px;
    padding-left: 20px;
}
.dltitle a
{
    float: right;
    margin-top: -37px;
    background: url(../../images/d03.jpg) no-repeat right top;
    height: 27px;
    padding: 10px 13px 0px 0px;
}

.dlcn
{
    height: 345px;
    background: url(../../images/d05.jpg) no-repeat left top;
}
.dllf
{
    float: left;
    padding: 20px 0px 0px 50px;
    width: 350px;
    /*background-image: url('../../images/dl.jpg');*/
}
.dllf li
{
    width: 330px;
    float: left;
    padding: 5px 0px;
}
.dllf li strong, .dlrt li strong
{
    display: block;
    font-size: 15px;
    font-weight: bold;
}
.dllf li span
{
    float: left;
    padding: 5px 0px 0px 33px;
    width: 60px;
    text-align:right;
}
.dlsr
{
    float: left;
    padding-left: 7px;
    background: url(../../images/d09.jpg) no-repeat left top;
}
.dlsr b
{
    display: block;
